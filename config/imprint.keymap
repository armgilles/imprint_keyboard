        #define KEY_LH_C2R4 F
        #define KEY_RH_C2R4 J
        #define KEY_RH_C3R4 K
        #define KEY_RH_C4R4 L
        #define KEY_RH_C5R4 P

        #define LEFT_PINKY_KEY  KEY_LH_C5R4
        #define LEFT_RINGY_KEY  KEY_LH_C4R4
        #define LEFT_MIDDY_KEY  KEY_LH_C3R4
